            <category>
            <pattern>MAZE</pattern>
            <template>
            <!-- starting room -->

            <think><set name="topic">ROOM05</set></think>

            <think><set name="seen00">UNSEEN</set></think>

            <think><set name="revision0">TRUE</set></think>

            <think><set name="seen01">UNSEEN</set></think>

            <think><set name="revision1">FALSE</set></think>

            <think><set name="seen02">UNSEEN</set></think>

            <think><set name="revision2">FALSE</set></think>

            <think><set name="seen03">UNSEEN</set></think>

            <think><set name="revision3">FALSE</set></think>

            <think><set name="seen04">UNSEEN</set></think>

            <think><set name="revision4">FALSE</set></think>

            <think><set name="seen05">UNSEEN</set></think>

            <think><set name="revision5">FALSE</set></think>

            <think><set name="seen06">UNSEEN</set></think>

            <think><set name="revision6">FALSE</set></think>

            <think><set name="LAMP">ROOM05</set></think>

            <think><set name="PIZZA">ROOM01</set></think>

            <srai>D4D8EFB81F48C9C INTERNALLOOK <get name="topic" /></srai>
            </template>
            </category>
<!-- ROOM00 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM00</pattern>
            <template>
                <!--  -->
                
                <condition name="seen00" value="UNSEEN">
                    
                </condition>

                <condition name="seen00" value="SEEN">
                    
                </condition >
                <srai> INTERNALLISTROOM00</srai>

                <think><set name="seen00">SEEN</set></think>
                <think><set name="topic">ROOM00</set></think>
            </template>

        </category>
<!-- ROOM01 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM01</pattern>
            <template>
                <!-- Room 1 - East room -->
                
                <condition name="seen01" value="UNSEEN">
                    Go west. Go East. Meta Room B. There is a door here on the east wall. This is the longer text that fills the screen when the room is visited for the first time. It may contain several sentences.
                </condition>

                <condition name="seen01" value="SEEN">
                    Room 1 - East room
                </condition >
                <srai> INTERNALLISTROOM01</srai>

                <think><set name="seen01">SEEN</set></think>
                <think><set name="topic">ROOM01</set></think>
            </template>

        </category>
<!-- ROOM02 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM02</pattern>
            <template>
                <!-- Room 2 - west room -->
                
                <condition name="seen02" value="UNSEEN">
                    Go east. Go west if passage is unlocked. This is the longer text that fills the screen when the room is visited for the first time. It may contain several sentences.
                </condition>

                <condition name="seen02" value="SEEN">
                    Room 2 - west room
                </condition >
                <srai> INTERNALLISTROOM02</srai>

                <think><set name="seen02">SEEN</set></think>
                <think><set name="topic">ROOM02</set></think>
            </template>

        </category>
<!-- ROOM03 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM03</pattern>
            <template>
                <!-- Room 3 - South room -->
                
                <condition name="seen03" value="UNSEEN">
                    go north. go south to Meta room. This is the longer text that fills the screen when the room is visited for the first time. It may contain several sentences.
                </condition>

                <condition name="seen03" value="SEEN">
                    Room 3 - South room
                </condition >
                <srai> INTERNALLISTROOM03</srai>

                <think><set name="seen03">SEEN</set></think>
                <think><set name="topic">ROOM03</set></think>
            </template>

        </category>
<!-- ROOM04 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM04</pattern>
            <template>
                <!-- Room 4 - Meta room  -->
                
                <condition name="seen04" value="UNSEEN">
                    Go south or north. Meta Room A. This is the longer text that fills the screen when the room is visited for the first time. It may contain several sentences.
                </condition>

                <condition name="seen04" value="SEEN">
                    Room 4 - Meta room 
                </condition >
                <srai> INTERNALLISTROOM04</srai>

                <think><set name="seen04">SEEN</set></think>
                <think><set name="topic">ROOM04</set></think>
            </template>

        </category>
<!-- ROOM05 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM05</pattern>
            <template>
                <!-- Room 5 - Start room  -->
                
                <condition name="seen05" value="UNSEEN">
                    Go east, west, or south. If you have visited the Meta room, you can go north. This is the longer text that fills the screen when the room is visited for the first time. It may contain several sentences.
                </condition>

                <condition name="seen05" value="SEEN">
                    Room 5 - Start room 
                </condition >
                <srai> INTERNALLISTROOM05</srai>

                <think><set name="seen05">SEEN</set></think>
                <think><set name="topic">ROOM05</set></think>
            </template>

        </category>
<!-- ROOM06 -->
        <category>
        <pattern>D4D8EFB81F48C9C INTERNALLOOK ROOM06</pattern>
            <template>
                <!-- The door is open. -->
                
                <condition name="seen06" value="UNSEEN">
                    The door opens with a swoosh.
                </condition>

                <condition name="seen06" value="SEEN">
                    The door is open.
                </condition >
                <srai> INTERNALLISTROOM06</srai>

                <think><set name="seen06">SEEN</set></think>
                <think><set name="topic">ROOM01</set></think>
            </template>

        </category>
<category>
            <pattern>
            INTERNALREJECT D4D8EFB81F48C9C
            </pattern>
            <template>
                <!-- condition name="move" value="TRUE" -->
                You cannot do that here.
                <think><set name="move">FALSE</set></think>
                <!-- /condition -->
            </template>
            </category>

<!-- simple look command -->
        <category>
        <pattern>
            LOOK
        </pattern>
        <template>
            <!-- one condition block for every room -->
            <condition name="topic" value="ROOM00">
                <think><set name="seen00">UNSEEN</set></think>
            </condition>
            <condition name="topic" value="ROOM01">
                <think><set name="seen01">UNSEEN</set></think>
            </condition>
            <condition name="topic" value="ROOM02">
                <think><set name="seen02">UNSEEN</set></think>
            </condition>
            <condition name="topic" value="ROOM03">
                <think><set name="seen03">UNSEEN</set></think>
            </condition>
            <condition name="topic" value="ROOM04">
                <think><set name="seen04">UNSEEN</set></think>
            </condition>
            <condition name="topic" value="ROOM05">
                <think><set name="seen05">UNSEEN</set></think>
            </condition>
            <condition name="topic" value="ROOM06">
                <think><set name="seen06">UNSEEN</set></think>
            </condition>
            <srai>D4D8EFB81F48C9C INTERNALLOOK <get name="topic" /></srai>
            </template>
            </category>
